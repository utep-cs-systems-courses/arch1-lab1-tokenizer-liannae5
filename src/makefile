#makefile for lab 1

#list of object files
OBJS= uimain.o tokenizer.o history.o

#flags
CFLAGS=-g -O3 # -g debug, -O3 for optimization

#first target
all: lab1

#prerequisites
lab1: $(OBJS)
	cc -o lab1 $(CFLAGS) $(OBJS)

#all obj files built from c files that include history.h and tokenizer.h
$(OBJS): history.h tokenizer.h

#deletes files (objs) generated by compilation
clean:
	rm -f *.o lab1

run: lab1
	./lab1
